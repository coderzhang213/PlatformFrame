workspace 'PlatformFrame'

project '../PlatformFrame/PlatformFrame.xcodeproj'
project '../PlatformFrame/NetWorkModule/NetWorkModule.xcodeproj'

source 'https://github.com/CocoaPods/Specs.git' #指定source源为master-

platform :ios, '10.0'

def pods

#定义导入的pods

pod 'AFNetworking'


end

target 'PlatformFrame' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  project '../PlatformFrame/PlatformFrame.xcodeproj'

end

target 'NetWorkModule' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  project '../PlatformFrame/NetWorkModule/NetWorkModule.xcodeproj'
  
  pods
  pod 'AFNetworking'


end

