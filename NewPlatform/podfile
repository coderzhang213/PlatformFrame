workspace 'NewPlatform'

project '../NewPlatform/NewPlatform.xcodeproj'
project '../NewPlatform/NetWorkFrame/NetWorkFrame.xcodeproj'
project '../NewPlatform/CommonClass/CommonClass.xcodeproj'
project '../NewPlatform/CommenTool/CommenTool.xcodeproj'

source 'https://github.com/CocoaPods/Specs.git' #指定source源为master-

platform :ios, '10.0'

def pods

#定义导入的pods



pod 'AFNetworking'
pod 'Texture'

end

target 'NetWorkFrame' do
  # Uncomment this line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!

  project '../NewPlatform/NetWorkFrame/NetWorkFrame.xcodeproj'
  
  pods
  pod 'AFNetworking'


end

target 'NewPlatform' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  project '../NewPlatform/NewPlatform.xcodeproj'

end

target 'CommonClass' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  # use_frameworks!
  project '../NewPlatform/CommonClass/CommonClass.xcodeproj'
  pods
  pod 'Texture'

end
